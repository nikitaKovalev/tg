  "scripts": {
    "*** Dev ***": "",
    "start": "npx nx serve client",
    "build:dev": "npx nx run client:build:development",
    "*** Production ***": "",
    "build:prod": "npx nx run client:build:production",
    "exec": "ts-node --project tsconfig.base.json",
    "postinstall": "npm run exec -- ./scripts/postinstall.ts",
    "lint": "npx nx affected:lint -t --all",
    "lint:all": "npx nx run-many -t lint --all --parallel",
    "prettier": "prettier '**/*.{svg,yml,js,ts,html,md,less,json}' --write --fix",
    "*** Release ***": "",
    "release": "standard-version -a --no-verify",
    "release:patch": "ts-node --project tsconfig.base.json ./scripts/release.ts --release-as patch",
    "release:minor": "ts-node --project tsconfig.base.json ./scripts/release.ts --release-as minor",
    "release:major": "ts-node --project tsconfig.base.json ./scripts/release.ts --release-as major"
  },
